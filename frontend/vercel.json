
{
  "rewrites": [
    { "source": "/css/(.*)", "destination": "/css/$1" },
    { "source": "/js/(.*)", "destination": "/js/$1" },
    { "source": "/Home/(.*)", "destination": "/js/Home/$1" },
    { "source": "/scss/(.*)", "destination": "/scss/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "cleanUrls": true
}
